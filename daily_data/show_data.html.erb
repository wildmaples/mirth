<%= content_tag(:ul) do %>
  <% @all_daily_data.each do |daily_data| %> 
    <%= content_tag(:li) do %>
      On this day <%= content_tag(:b, daily_data.date) %>, <%= daily_data.step_count %>, <%= daily_data.notes %>
    <% end %>
  <% end %>
<% end %>

<%= form_tag("/add/data") do %>
  <%= content_tag(:p) do %>
    <%= label_tag("date", "Date") %>
    <%= date_field_tag("date") %>
  <% end %>

  <%= content_tag(:p) do %>
    <%= label_tag("step_count", "Step Count") %>
    <%= number_field_tag("step_count") %>
  <% end %>

  <%= content_tag(:p) do %>
    <%= label_tag("notes", "Notes") %>
    <%= text_area_tag("notes") %>
  <% end %>
  
  <%= button_tag("Submit daily data") %>
<% end %>
